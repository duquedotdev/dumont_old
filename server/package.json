{
  "name": "Demoiselle",
  "version": "0.0.1",
  "description": "Backend do Dumont (software open-source de centro de instrução de aviação)",
  "main": "index.js",
  "repository": "https://github.com/duquedotdev/Dumont",
  "author": "Felipe Duque",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "NODE_ENV=development npm-run-all -p dev:*",
    "dev:server": "nodemon src/server.js",
    "dev:queue": "nodemon --inspect src/queue.js",
    "debug": "nodemon --inspect src/queue.js",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "start": "node dist/server.js",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test jest -o --watch --runInBand --detectOpenHandles",
    "postest": "NODE_ENV=test sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@sentry/node": "^5.15.5",
    "accesscontrol": "^2.2.1",
    "bcryptjs": "^2.4.3",
    "bull": "^3.13.0",
    "bull-board": "^0.7.0",
    "celebrate": "^12.1.0",
    "cross-env": "^7.0.2",
    "date-fns": "^2.0.0-beta.5",
    "dotenv": "^8.2.0",
    "eslint-plugin-security": "^1.4.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "js-sha3": "^0.8.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.20",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.6",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.11",
    "sqlite3": "^4.2.0",
    "youch": "^2.0.10"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/jest": "^25.2.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "sequelize-cli": "^5.5.1",
    "sucrase": "^3.13.0",
    "supertest": "^4.0.2"
  }
}
